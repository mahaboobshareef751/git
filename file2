import java.util.Scanner;
public class reverse {
	

	
	static int getpal(int n)
	{   
		int rev = 0;
		while(n!=0){
		    int d = n%10;
		    rev = rev*10 + d;
		    n= n%10;
	      }
			
		return rev;

	}
	public static void main(String[] args)
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("enter the no:");
		int i = sc.nextInt();
		int b = getpal(i);
	
		System.out.println("true");
	}
	}



